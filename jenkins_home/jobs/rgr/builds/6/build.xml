    <script>pipeline {
    agent any
    environment {
        DOCKER_COMPOSE_VERSION = &apos;1.29.2&apos;
        DOCKER_COMPOSE_BIN = &quot;${env.HOME}/docker-compose&quot; // Путь для установки docker-compose
    }
    stages {
        stage(&apos;Checkout&apos;) {
            steps {
                git branch: &apos;main&apos;, url: &apos;https://github.com/grogu27/jenkins_avs.git&apos;
            }
        }
        stage(&apos;Install Docker Compose&apos;) {
            steps {
                script {
                    // Проверка, установлен ли docker-compose
                    if (!fileExists(&quot;${DOCKER_COMPOSE_BIN}&quot;)) {
                        echo &apos;docker-compose не найден, установка...&apos;
                        sh &quot;&quot;&quot;
                            curl -L https://github.com/docker/compose/releases/download/${DOCKER_COMPOSE_VERSION}/docker-compose-Linux-x86_64 -o ${DOCKER_COMPOSE_BIN}
                            chmod +x ${DOCKER_COMPOSE_BIN}
                        &quot;&quot;&quot;
                    } else {
                        echo &apos;docker-compose уже установлен.&apos;
                    }
                }
            }
        }
        stage(&apos;Build and Deploy with Docker Compose&apos;) {
            steps {
                script {
                    // Перейдем в каталог, где расположен docker-compose.yml
                    dir(&apos;.&apos;) {
                        // Запуск сборки и деплоя с помощью docker-compose
                        sh &quot;${DOCKER_COMPOSE_BIN} -f docker-compose.yml up -d --build&quot;
                    }
                }
            }
        }
    }
    triggers {
